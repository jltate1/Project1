<apex:page controller="PastDeliveries_CC">
<h1>Past Deliveries</h1>
<apex:pageBlock>
    <apex:pageBlockTable value="{! mailDelivered}" var="mail" id="mailList">
                    <apex:column headerValue="Tracking Number">
                        <apex:outputLink value="/{! mail.ID}">
                            {! mail.Name}
                        </apex:outputLink>
                    </apex:column>
                    <!-- <apex:column headerValue="Delivery Date"> -->
                    <apex:column headerValue="Status Number">
                            <!-- {! mail.Last_Status_Update_Date__c} -->
                        {! mail.Delivery_Status__c}
                    </apex:column>
    </apex:pageBlockTable>
</apex:pageBlock>

</apex:page>